<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.pt_assistant.InjuryLookupActivity"
    android:background="@drawable/custom_background" >

    <ListView
        android:id="@+id/injuryList"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_above="@+id/backObjective"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        style="@style/PTFont.Dark" >

    </ListView>

    <Button
        android:id="@+id/backObjective"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:onClick="backObjective"
        android:text="Back"
        android:background="@drawable/custom_button"
        android:textAppearance="@style/PTFont.Dark" />

</RelativeLayout>
